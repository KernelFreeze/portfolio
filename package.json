        "pages:dev": "wrangler pages dev .vercel/output/static --compatibility-date=2023-12-02 --compatibility-flag=nodejs_compat"
    },
    "dependencies": {
        "@nextui-org/react": "^2.2.9",
        "framer-motion": "^10.16.12",
        "next": "14.0.3",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@cloudflare/next-on-pages": "1",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.0.3",
        "eslint-plugin-next-on-pages": "^1.8.0",
        "eslint-plugin-prettier": "^5.0.1",
        "postcss": "^8",
        "prettier": "3.1.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5",
        "vercel": "^32.6.1",
        "wrangler": "^3.18.0"
    },
    "packageManager": "pnpm@8.11.0"
