{
    "name": "portfolio",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "pages:build": "next-on-pages",
        "pages:deploy": "pnpm pages:build && wrangler pages deploy .vercel/output/static",
